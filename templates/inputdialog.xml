<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<formTemplate msgid="{{msgid}}">
		<title>{{title}}</title>
		<description>{{description}}</description>                
		<textField id="text" secure="{{secure}}">{{placeholder}}</textField>
		<checkbox on="true"/>
		<footer>
			<button documentURL="{{url}}">
				<text>{{button}}</text>
			</button>
		</footer>		 
	</formTemplate>
</document>