<?xml version="1.0" encoding="UTF-8" ?>
<document>
	<formTemplate msgid="{{msgid}}">
		<title>{{title}}</title>
		<description>{{description}}</description>                
		<row>
			<text>hello</text>
			<textField id="text" secure="{{secure}}">{{placeholder}}</textField>
		</row>	
